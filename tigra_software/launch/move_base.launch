<?xml version="1.0" encoding="UTF-8"?>
<launch>
     

	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        
        <rosparam file="$(find tigra_software)/config/move_base/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
        
        <rosparam file="$(find tigra_software)/config/move_base/costmap_common_params.yaml" command="load" ns="local_costmap" />
        
        <rosparam file="$(find tigra_software)/config/move_base/local_costmap_params.yaml" command="load" />
        
        <rosparam file="$(find tigra_software)/config/move_base/global_costmap_params.yaml" command="load" /> 
        
	<rosparam file="$(find tigra_software)/config/move_base/dwa_local_planner.yaml" command="load" />
<!--  	<rosparam file="$(find tigra_software)/config/move_base/teb_local_planner.yaml" command="load" /> -->

	<remap from="cmd_vel" to="tigra/cmd_vel"/>
 </node>


</launch> 
